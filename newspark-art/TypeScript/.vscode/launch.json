{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Cucumber-js: Run All",
      "program": "${workspaceFolder}/node_modules/.bin/cucumber-js",
      "args": [
        "--format", "html:reports/cucumber-report.html",
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    }
  ]
}

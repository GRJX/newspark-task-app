{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Behave: current file",
            "type": "debugpy",
            "request": "launch",
            "module": "behave",
            "console": "integratedTerminal",
            "args": [
                "${file}"
            ]
        },
        {
            "name": ".NET Test Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": [
                "test",
            ],
            "cwd": "${workspaceFolder}/newspark-art/CSharp",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
    ]
}